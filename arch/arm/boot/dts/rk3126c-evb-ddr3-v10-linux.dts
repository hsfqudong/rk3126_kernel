};

&sdio {
	bus-width = <4>;
	max-frequency = <50000000>;
	cap-sd-highspeed;
	supports-sdio;
	ignore-pm-notify;
	keep-power-in-suspend;
	supports-rk912;
	/delete-property/ non-removable;
	status = "okay";
};

&sdmmc {
	bus-width = <4>;
	cap-sd-highspeed;
	supports-sd;
	card-detect-delay = <800>;
	ignore-pm-notify;
	keep-power-in-suspend;
	cd-gpios = <&gpio2 3 GPIO_ACTIVE_LOW>; /* CD GPIO */
