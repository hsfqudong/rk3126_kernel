		display-timings {
			native-mode = <&timing0>;

			timing0: timing0 {
				clock-frequency = <45000000>;
				hactive = <1024>;
				vactive = <600>;
				hback-porch = <10>;
				hfront-porch = <210>;
				vback-porch = <10>;
				vfront-porch = <22>;
				hsync-len = <30>;
