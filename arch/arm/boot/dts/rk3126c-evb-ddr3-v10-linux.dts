&pinctrl {
	pmic {
		pmic_int_l: pmic-int-l {
			rockchip,pins =
				<0 RK_PA0 0 &pcfg_pull_default>;
		};
	};

	buttons {
		pwr_btn: pwr-btn {
		   rockchip,pins = 
				<2 RK_PC0 0 &pcfg_pull_none>;
		};
		
		gpio_key_pin: gpio-key-pin {
		   rockchip,pins = 
				<1 RK_PB1 RK_FUNC_GPIO &pcfg_pull_default>,
			    <2 RK_PA0 RK_FUNC_GPIO &pcfg_pull_default>,
			    <2 RK_PA1 RK_FUNC_GPIO &pcfg_pull_default>,
			    <2 RK_PA2 RK_FUNC_GPIO &pcfg_pull_default>,
			    <2 RK_PA4 RK_FUNC_GPIO &pcfg_pull_default>,
			    <3 RK_PB3 RK_FUNC_GPIO &pcfg_pull_default>,
			    <2 RK_PC3 RK_FUNC_GPIO &pcfg_pull_default>,
			    <2 RK_PC2 RK_FUNC_GPIO &pcfg_pull_default>,
			    <2 RK_PB4 RK_FUNC_GPIO &pcfg_pull_default>,
			    <2 RK_PB5 RK_FUNC_GPIO &pcfg_pull_default>,
			    <3 RK_PB2 RK_FUNC_GPIO &pcfg_pull_default>;
	};
};

	usb2 {
		usb_hub_h: usb-hub-h {
			rockchip,pins =
				<0 RK_PA1 0 &pcfg_pull_none>;
		};
	};
	wireless-bluetooth {
		uart1_rts_gpio: uart1-rts-gpio {
			rockchip,pins = <1 RK_PB3 RK_FUNC_GPIO &pcfg_pull_none>;
		};
	};

	sdio-pwrseq {
		wifi_enable_h: wifi-enable-h {
			rockchip,pins = <2 RK_PB2 RK_FUNC_GPIO &pcfg_pull_none>;
		};
	};
};

&pwm0 {
	status = "okay";
